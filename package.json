{
  "name": "reactNativeOsBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest -c jest.config.json",
    "precommit": "lint-staged",
    "lint": "eslint src/**/*.js",
    "flow": "flow"
  },
  "rnpm": {
    "assets": [
      "src/assets/fonts/Montserrat-Bold",
      "src/assets/fonts/Montserrat-Light",
      "src/assets/fonts/Montserrat-Regular",
      "src/assets/fonts/SpaceMono-Regular"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint src/**/*.js"
      ]
    },
    "ignore": [
      "**/flow-typed/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@feathersjs/authentication-client": "^1.0.9",
    "@feathersjs/feathers": "^3.3.0",
    "@feathersjs/rest-client": "^1.4.6",
    "axios": "^0.18.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "intl": "^1.2.5",
    "react": "16.6.3",
    "react-intl": "^2.7.2",
    "react-native": "0.58.1",
    "react-native-elements": "1.0.0",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-vector-icons": "^6.2.0",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-polyfill": "^6.26.0",
    "detox": "^12.6.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-native": "^4.0.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "0.91.0",
    "husky": "^1.2.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "nock": "^10.0.4",
    "prettier": "^1.15.3",
    "prettier-eslint": "^8.8.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.6.3",
    "redux-test-utils": "^0.3.0",
    "regenerator-runtime": "^0.13.1",
    "remote-redux-devtools": "0.5.0",
    "sinon": "^7.3.2"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/DetoxTest.app",
        "build": "xcodebuild -project ios/DetoxTest.xcodeproj -scheme DetoxTest -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_28_x86"
      }
    }
  }
}
